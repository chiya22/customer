<!DOCTYPE html>
<html>

<head>
  <title>入居番号管理</title>
  <%- include("../common/stylesheets.ejs") %>
</head>

<body>
  <%- include("../common/header.ejs", {current: 'master'}) %>
  <div class="main">
    <div class="main__wrapper">
      <div class="area-admin-link">
        <a class="btn-base" href="/admin/nyukyos/insert">登録</a>
      </div>
      <div class="admin__nyukyos">
          <% if (nyukyos) { %>
          <%  nyukyos.forEach( nyukyo => { %>
          <div class="admin_nyukyo">
            <a class="btn-link" href="/admin/nyukyos/update/<%= nyukyo.id %>">
              <%= nyukyo.id %><span class="dateinfo-small">　開始：<%= nyukyo.ymd_start.slice(0,4) %>.<%= nyukyo.ymd_start.slice(4,6) %>.<%= nyukyo.ymd_start.slice(6,8) %>　終了：<%= nyukyo.ymd_end.slice(0,4) %>.<%= nyukyo.ymd_end.slice(4,6) %>.<%= nyukyo.ymd_end.slice(6,8) %>　</span>
              <%= nyukyo.id_company %><%= nyukyo.name_company %>
            </a>
          </div>
          <%  }) %>
          <% } %>
      </div>
      <!-- /.admin_nyukyo -->
    </div>
    <!-- /.main__wrapper -->
  </div>
  <!-- /.main -->
</body>
<%- include("../common/javascripts.ejs") %>
</html>