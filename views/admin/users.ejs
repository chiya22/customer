<!DOCTYPE html>
<html>

<head>
  <title>ユーザ管理</title>
  <%- include("../common/stylesheets.ejs") %>
</head>

<body>
  <%- include("../common/header.ejs") %>
  <div class="main">
    <div class="main__wrapper">
      <div class="area-link">
        <a class="btn-base" href="/admin/users/insert">登録</a>
      </div>
        <div class="admin__user">
          <% if (users) { %>
          <%  users.forEach( user => { %>
          <div>
            <a class="btn-link" href="/admin/users/update/<%= user.id %>">
              <%= user.name %>（<%= user.id %>）
            </a>
          </div>
          <%  }) %>
          <% } %>
      </div>
      <!-- /.admin__user -->
    </div>
    <!-- /.main__wrapper -->
  </div>
  <!-- /.main -->
</body>
<%- include("../common/javascripts.ejs") %>
</html>